    "compilerOptions": {
        "incremental": true,
        "target": "es2016",
        "module": "commonjs",
        "outDir": "build",
        "rootDir": "src",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true
    }
}
